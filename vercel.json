{
    "version": 2,
    "builds": [
        {
          "src": "main.py", 
          "use": "@vercel/python",
          "config": {
            "runtime": "python3.10" 
          }
        }
      ],
    "installCommand": "pip install -r requirements.txt", 
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ]
  }